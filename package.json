{
  "scripts": {
    "migrate": "node main.js"
  },
  "type": "module",
  "dependencies": {
    "@bull-board/express": "^5.7.1",
    "bullmq": "^4.7.0",
    "dotenv": "^16.3.1",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.1"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "simple-import-sort",
      "import"
    ],
    "rules": {
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error",
      "import/first": "error",
      "import/newline-after-import": "error",
      "import/no-duplicates": "error"
    }
  },
  "prettier": {}
}
